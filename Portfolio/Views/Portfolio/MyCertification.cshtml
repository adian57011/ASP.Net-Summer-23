
@{
    ViewBag.Title = "MyCertification";
    Layout = "~/Views/Shared/_PortfolioLayout.cshtml";
}

<h2>My Certifications</h2>
<table class="table table-bordered">
    <tr>
        <th>Degree</th>
        <th>Cgpa/Gpa</th>
        <th>Institute</th>
        <th>Year</th>
    </tr>
    @foreach(var item in ViewBag.Edus)
            {
                <tr>
                    <td>@item.Degree</td>
                    <td>@item.Cgpa</td>
                    <td>@item.Institute</td>
                    <td>@item.Year</td>
                </tr>
            }
           
</table>

